<div class="container-fluid index-main">
	<div class="header-top container">
		<h1 class="font-times-header text-uppercase">Categories</h1>
		<p>All you need and more to get hitched without a hitch.</p>
		<hr>
	</div>
	<div style="padding: 50px 20px 20px 20px;">
		{{#each model as |category|}}
		{{#link-to 'category' category.id}}
		<div class="col-sm-3 height-3 category-outer-square col-xs-4">
			<div class="category-square" style={{category.style}}>
				<i class={{category.icon}}>{{category.iconName}}</i>
				<p class="cat-heading">{{category.name}}</p>
			</div>
		</div>{{/link-to}}
		{{/each}}
