<div class="header-top">
	<h1>Listings</h1><hr class="style-two"><h3>Click the heart to favourite!</h3>
</div>

<div class="container">
	<div class="row">
	    {{#each model as |cat-item|}}
		<div class="col-lg-4 col-md-6">
			<div class="category-block">
				<h2>{{cat-item.name}}</h2>
				<div class="text-center cat-img-container">
					<img src="{{cat-item.imageURL}}" class="img-responsive cat-image">
				</div>
				<button type="button" class="btn btn-danger cat-heart-button"><i class="fa fa-heart-o"></i></button>
			</div>
		</div>
	    {{/each}}
	</div>
</div>