<div class="header-top"><h1>Users</h1></div>
<table class="table table-bordered table-striped">
    <thead>
      <tr>
          <th>Full Name</th>
          <th>E-mail</th>
          <th>Facebook</th>
      </tr>
    </thead>
    <tbody>
    {{#each model as |user|}}
        <tr>
            <td>{{user.name}} {{user.surname}}</td>
            <td>{{user.email}}</td>
            <td><a href="http://facebook.com/{{user.id}}">View Profile</a></td>
        </tr>
    {{/each}}

    </tbody>
</table>